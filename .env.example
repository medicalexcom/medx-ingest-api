# URL of your renderer (the medx-render-api deployment)
RENDER_API_URL=https://<https://medx-render-api.onrender.com>/ 
# optional if your renderer protects the /render endpoint
RENDER_API_TOKEN=
# tuning knobs (sane defaults already in code)
MIN_IMG_PX=200
EXCLUDE_PNG=false
RENDER_TIMEOUT_MS=45000
TOTAL_TIMEOUT_MS=60000
ENABLE_CACHE=true
